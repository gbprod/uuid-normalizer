{
    "name": "gbprod/uuid-normalizer",
    "description": "Normalizer to serialize Ramsey Uuid with Symfony Serializer",
    "type": "library",
    "autoload": {
        "psr-4": { "GBProd\\UuidNormalizer\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Tests\\GBProd\\UuidNormalizer\\": "tests/" }
    },
    "require": {
        "php": ">=5.5.9",
        "ramsey/uuid": "^3.0|^3.4",
        "symfony/serializer": "^2.3|^3.1"
    },
    "require-dev": {
        "phpunit/phpunit": ">=4.8"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "GBProd",
            "email": "contact@gb-prod.fr"
        }
    ],
    "minimum-stability": "stable"
}
