{
  "name": "gbprod/uuid-normalizer",
  "description": "Normalizer to serialize Ramsey Uuid with Symfony Serializer",
  "type": "library",
  "autoload": {
    "psr-4": {
      "GBProd\\UuidNormalizer\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\GBProd\\UuidNormalizer\\": "tests/"
    }
  },
  "require": {
    "php": "^7.2|^8.0",
    "ramsey/uuid": "^4.0",
    "symfony/serializer": "^3.4|^4.3|^5.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.0",
    "symfony/property-access": "^3.4|^4.3|^5.0",
    "phpstan/phpstan": "^0.12.37"
  },
  "license": "WTFPL",
